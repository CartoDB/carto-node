{
  "source": {
    "include": ["src"]
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
      "plugins": ["transform-object-rest-spread"],
       "extensions": ["js", "es6", "jsx"],
       "presets": ["env"]
  },
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "useLongnameInNav": true
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/public"
  }
}
